<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.tab00.HeadOfficeActivity">

        <androidx.core.widget.NestedScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <androidx.appcompat.widget.AppCompatImageButton
                    android:id="@+id/btnBack"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:src="@drawable/icon_nav_back_black"
                    android:scaleType="fitXY"
                    android:background="@drawable/effect_ripple_oval"
                    android:layout_marginTop="12dp"
                    android:layout_marginStart="14dp"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintWidth_percent="0.056"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"/>

                <View
                    android:id="@+id/viewGuide"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintWidth_percent="0.88"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeDate"
                    android:layout_width="0dp"
                    android:layout_height="24dp"
                    android:layout_marginTop="72dp"
                    android:autoSizeMaxTextSize="16dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/main_black"
                    android:textSize="18dp"
                    android:textStyle="bold"
                    android:gravity="center_vertical|start"
                    app:layout_constraintStart_toStartOf="@+id/viewGuide"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintWidth_percent="0.23" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeTitle"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:autoSizeMaxTextSize="16dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/main_black"
                    android:textSize="16dp"
                    android:gravity="center_vertical|start"
                    app:layout_constraintBottom_toBottomOf="@+id/txtHeadOfficeDate"
                    app:layout_constraintEnd_toEndOf="@+id/viewGuide"
                    app:layout_constraintTop_toTopOf="@+id/txtHeadOfficeDate"
                    app:layout_constraintWidth_percent="0.62" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txt03"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="13dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:visibility="invisible"
                    app:layout_constraintTop_toBottomOf="@+id/txtHeadOfficeDate"
                    app:layout_constraintStart_toStartOf="@+id/viewGuide"
                    app:layout_constraintWidth_percent="0.21"
                    app:layout_constraintDimensionRatio="1:0.522"/>


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMenu00"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txt03"
                    app:layout_constraintBottom_toBottomOf="@+id/txt03"
                    app:layout_constraintEnd_toStartOf="@+id/txtHeadOfficeMenu01"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMenu01"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txt03"
                    app:layout_constraintBottom_toBottomOf="@+id/txt03"
                    app:layout_constraintEnd_toStartOf="@+id/txtHeadOfficeMenu02"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMenu02"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txt03"
                    app:layout_constraintBottom_toBottomOf="@+id/txt03"
                    app:layout_constraintEnd_toStartOf="@+id/txtHeadOfficeMenu03"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMenu03"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txt03"
                    app:layout_constraintBottom_toBottomOf="@+id/txt03"
                    app:layout_constraintEnd_toEndOf="@+id/viewGuide"
                    app:layout_constraintWidth_percent="0.11"/>


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtSalesForYear"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    app:layout_constraintDimensionRatio="1:0.522"
                    app:layout_constraintStart_toStartOf="@+id/viewGuide"
                    app:layout_constraintTop_toBottomOf="@+id/txt03"
                    app:layout_constraintWidth_percent="0.21" />

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtSalesForYearMenu00"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txtSalesForYear"
                    app:layout_constraintBottom_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintEnd_toStartOf="@+id/txtSalesForYearMenu01"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtSalesForYearMenu01"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txtSalesForYear"
                    app:layout_constraintBottom_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintEnd_toStartOf="@+id/txtSalesForYearMenu02"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtSalesForYearMenu02"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txtSalesForYear"
                    app:layout_constraintBottom_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintEnd_toStartOf="@+id/txtSalesForYearMenu03"
                    app:layout_constraintWidth_percent="0.186"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtSalesForYearMenu03"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:gravity="center"
                    android:textColor="@color/main_black"
                    android:autoSizeMaxTextSize="12dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    app:layout_constraintTop_toTopOf="@+id/txtSalesForYear"
                    app:layout_constraintBottom_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintEnd_toEndOf="@+id/viewGuide"
                    app:layout_constraintWidth_percent="0.11"/>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/stroke_grey"
                    app:layout_constraintTop_toBottomOf="@+id/txt03"
                    app:layout_constraintStart_toStartOf="@+id/txt03"
                    app:layout_constraintEnd_toEndOf="@+id/txtHeadOfficeMenu03"/>

                <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/stroke_grey"
                    app:layout_constraintTop_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintStart_toStartOf="@+id/txtSalesForYear"
                    app:layout_constraintEnd_toEndOf="@+id/txtSalesForYearMenu03"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMonthSales"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="14dp"
                    android:autoSizeMaxTextSize="14dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/main_black"
                    app:layout_constraintTop_toBottomOf="@+id/txtSalesForYear"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintDimensionRatio="1:0.0526"
                    app:layout_constraintWidth_percent="0.88" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/headOfficeSalesRecyclerView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:overScrollMode="never"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintTop_toBottomOf="@+id/txtHeadOfficeMonthSales"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/txtHeadOfficeMonthProfit"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginTop="14dp"
                    android:autoSizeMaxTextSize="14dp"
                    android:autoSizeMinTextSize="10dp"
                    android:autoSizeStepGranularity="1dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/main_black"
                    app:layout_constraintTop_toBottomOf="@+id/headOfficeSalesRecyclerView"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintDimensionRatio="1:0.0526"
                    app:layout_constraintWidth_percent="0.88" />

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/headOfficeRevenueRecyclerView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="40dp"
                    android:overScrollMode="never"
                    app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                    app:layout_constraintTop_toBottomOf="@+id/txtHeadOfficeMonthProfit"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"/>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
